{
	"name": "@sigan-io/lambda-layers",
	"version": "1.0.0",
	"scripts": {
		"build": "npm run build:prod && npm run build:dev",
		"build:prod": "earthly +wp-php",
		"build:dev": "earthly +wp-php-dev",
		"publish": "npm run publish:prod && npm run publish:dev",
		"publish:prod": "earthly --push +wp-php",
		"publish:dev": "earthly --push +wp-php-dev",
		"publish:layer": "earthly +wp-php-layer"
	}
}
